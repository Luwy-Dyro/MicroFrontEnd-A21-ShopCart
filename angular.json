						},
						"development": {
							"buildTarget": "mfe-header:esbuild:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"headers": {
							"Access-Control-Allow-Origin": "*"
						}
					}
				}
			}
		},
