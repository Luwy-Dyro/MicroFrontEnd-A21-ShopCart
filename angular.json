						}
					}
				}
			}
		},
		"cart-state": {
			"projectType": "library",
			"root": "projects/cart-state",
			"sourceRoot": "projects/cart-state/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular/build:ng-packagr",
					"configurations": {
						"production": {
							"tsConfig": "projects/cart-state/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/cart-state/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular/build:unit-test",
					"options": {
						"tsConfig": "projects/cart-state/tsconfig.spec.json"
					}
				}
			}
		},
		"ui-kit": {
			"projectType": "library",
			"root": "projects/ui-kit",
			"sourceRoot": "projects/ui-kit/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular/build:ng-packagr",
					"configurations": {
						"production": {
							"tsConfig": "projects/ui-kit/tsconfig.lib.prod.json"
